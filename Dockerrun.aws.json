{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "freeswitch",
      "image": "pincambodia/somleng-freeswitch",
      "essential": true,
      "memory": 750,
      "portMappings": [
        {
          "hostPort": 5222,
          "containerPort": 5222,
          "protocol": "tcp"
        },
        {
          "hostPort": 5060,
          "containerPort": 5060,
          "protocol": "udp"
        }
      ],
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "awslogs-freeswitch",
              "awslogs-region": "ap-southeast-1",
              "awslogs-stream-prefix": "awslogs-freeswitch"
          }
      },
      "environment": [
        {
          "name": "FREESWITCH_CONFIG_S3_PATH",
          "value": "s3://somlengsecrets/freeswitch/conf"
        }
      ]
    }
  ]
}
