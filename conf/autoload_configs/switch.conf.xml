<!-- https://freeswitch.org/stash/projects/FS/repos/freeswitch/browse/conf/vanilla/autoload_configs/switch.conf.xml -->

<configuration name="switch.conf" description="Core Configuration">
  <settings>
    <param name="colorize-console" value="$${core_colorize_console}"/>
    <param name="loglevel" value="$${core_loglevel}"/>

    <!-- Store the database in memory to avoid disk writes -->
    <param name="core-db-name" value="$${core_db_name}" />

    <param name="max-sessions" value="$${core_max_sessions}"/>
    <param name="sessions-per-second" value="$${core_sessions_per_second}"/>
    <param name="rtp-start-port" value="$${core_rtp_start_port}"/>
    <param name="rtp-end-port" value="$${core_rtp_end_port}"/>
  </settings>
</configuration>
