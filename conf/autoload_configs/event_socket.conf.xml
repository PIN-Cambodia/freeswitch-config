<!-- https://freeswitch.org/stash/projects/FS/repos/freeswitch/browse/src/mod/event_handlers/mod_event_socket/conf/autoload_configs/event_socket.conf.xml -->
<configuration name="event_socket.conf" description="Socket Client">
  <settings>
    <param name="nat-map" value="$${mod_event_socket_nat_map}"/>
    <param name="listen-ip" value="$${mod_event_socket_listen_ip}"/>
    <param name="listen-port" value="$${mod_event_socket_listen_port}"/>
    <param name="password" value="$${mod_event_socket_password}"/>
    <param name="apply-inbound-acl" value="$${mod_event_socket_apply_inbound_acl}"/>
  </settings>
</configuration>
