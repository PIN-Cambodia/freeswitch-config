<include>
  <context name="default">
    <extension name="adhearsion">
      <condition field="destination_number" expression="^2441$">
        <action application="rayo"/>
      </condition>
    </extension>
    <extension name="SimulateProductionCall">
      <condition field="destination_number" expression="^2442$">
        <action application="bridge" data="sofia/external/2442@freeswitch-staging.chibitxt.me"/>
      </condition>
    </extension>
    <extension name="TestExternalSipGateway">
      <condition field="destination_number" expression="^2443$">
        <action application="export" data="sip_from_uri=$${outgoing_did_number}@chibitxt.me"/>
        <action application="bridge" data="sofia/gateway/didlogic/$${test_number}"/>
      </condition>
    </extension>
  </context>
</include>
