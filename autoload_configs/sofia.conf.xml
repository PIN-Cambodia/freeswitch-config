<configuration name="sofia.conf">
  <profiles>
    <profile name="external">
      <settings>
        <param name="ext-rtp-ip" value="$${external_ip}"/>
        <param name="ext-sip-ip" value="$${external_ip}"/>

        <param name="rtp-ip" value="$${local_ip_v4}"/>
        <param name="sip-ip" value="$${local_ip_v4}"/>

        <param name="bind-params" value="transport=udp"/>

        <param name="inbound-codec-prefs" value="$${global_codec_prefs}"/>
        <param name="outbound-codec-prefs" value="$${global_codec_prefs}"/>

        <param name="context" value="default"/>
        <param name="disable-register" value="true"/>
        <param name="rtp-timer-name" value="soft"/>
        <param name="apply-inbound-acl" value="domains"/>
      </settings>

      <gateways>
        <gateway name="didlogic">
          <param name="username" value="$${didlogic_username}"/>
          <param name="realm" value="$${didlogic_realm}"/>
          <param name="password" value="$${didlogic_password}"/>
          <param name="register" value="true"/>
          <param name="context" value="public"/>
          <param name="caller-id-in-from" value="true"/>
          <param name="sip_cid_type" value="none"/>
        </gateway>
      </gateways>
    </profile>
  </profiles>
</configuration>
